<div class="dpl-react-app-container--page">
    <div {{ attributes }}>
        <div class="content-list-page">
            <h1 class="content-list-page__heading">
                {{ attributes['data-favorites-list-header-text']|raw }}
            </h1>
            <div class="ssc">
                <div class="ssc-line w-10 my-32">&nbsp;</div>
            </div>
            <ul class="content-list">
                {% for i in 1..5 %}
                <li>
                    <article class="card-list-item ssc">
                        <div class="ssc-square cover--size-small">&nbsp;</div>
                        <div class="ssc-wrapper">
                            <div class="ssc-head-line w-60 mb"></div>
                            <div class="ssc-line w-60 mbs">&nbsp;</div>
                            <div class="ssc-line w-60 mbs">&nbsp;</div>
                        </div>
                    </article>
                </li>
                {% endfor %}
            </ul>
        </div>
    </div>
</div>

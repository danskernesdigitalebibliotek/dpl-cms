parameters:
  level: max
  paths:
    - web/modules/custom
    - web/themes/custom
    - tests/behat/context
  ignoreErrors:
    - '#Unsafe usage of new static\(\).#'
    # Drupal Form API makes extensive use for arrays which we cannot provide
    # more detailed typing of.
    - '#.*\:\:(build|submit)Form\(\) .* no value type specified in iterable type array\.#'

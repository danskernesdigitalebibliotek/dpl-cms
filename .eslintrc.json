{
  "extends": [
    "plugin:drupal-contrib/9.x"
  ],
  "rules": {
    "no-console": ["error", { "allow": ["debug", "warn", "error"] }]
  },
  "ignorePatterns": [
    "**/web/core/",
    "**/web/libraries/",
    "**/web/modules/contrib/",
    "**/web/sites/**/files/",
    "**/web/themes/custom/novel/assets/",
    "**/web/themes/contrib/",
    "**/web/modules/local/",
    "**/web/modules/custom/dpl_example_content/content/",
    "**/web/modules/custom/dpl_static_content/content/"
  ]
}
